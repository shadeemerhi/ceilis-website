    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "date-fns": "^2.29.3",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "formik": "^2.2.9",
    "next": "13.3.0",
    "nodemailer": "^6.9.1",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-toastify": "^9.1.2",
    "tailwindcss": "3.3.1",
    "ts-node": "^10.9.1",
